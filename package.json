{
  "name": "express-nuxt",
  "version": "1.0.0",
  "description": "Express &amp; Nuxt",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently -n TS,Serve \"npm:watch:tsc\" \"npm:watch:serve\"",
    "watch:tsc": "npm run build:tsc -- --watch",
    "watch:serve": "nodemon --watch dist dist/server",
    "prebuild": "rm -rf dist && rm -rf .nuxt",
    "build": "npm run build:tsc && npm run build:nuxt",
    "build:tsc": "npm run lint && tsc -p tsconfig.server.json",
    "build:nuxt": "nuxt build",
    "start": "node dist/server",
    "lint": "tslint -p tsconfig.json",
    "test": "npm run lint && jest",
    "watch:test": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gongzza/express-nuxt.git"
  },
  "keywords": [
    "express",
    "nuxt"
  ],
  "author": "gongzza <gz.us.to@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gongzza/express-nuxt/issues"
  },
  "homepage": "https://github.com/gongzza/express-nuxt#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "nuxt": "^1.4.2",
    "reflect-metadata": "^0.1.12",
    "routing-controllers": "^0.7.7",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/morgan": "^1.7.35",
    "@types/supertest": "^2.0.5",
    "concurrently": "^3.6.1",
    "jest": "^23.5.0",
    "node-mocks-http": "^1.7.0",
    "nodemon": "^1.18.3",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^7.1.0"
  }
}
