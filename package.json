{
  "name": "express-nuxt",
  "version": "1.0.0",
  "description": "Express &amp; Nuxt",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build:tsc && concurrently -n TS,Serve \"npm:watch:tsc\" \"npm:watch:serve\"",
    "build:tsc": "npm run lint && tsc",
    "watch:tsc": "tsc -w",
    "watch:serve": "nodemon --watch dist dist/bin/server",
    "lint": "tslint -p .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gongzza/express-nuxt.git"
  },
  "keywords": [
    "express",
    "nuxt"
  ],
  "author": "gongzza <gz.us.to@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gongzza/express-nuxt/issues"
  },
  "homepage": "https://github.com/gongzza/express-nuxt#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "nuxt": "^1.4.2",
    "reflect-metadata": "^0.1.12",
    "routing-controllers": "^0.7.7",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/express": "^4.16.0",
    "concurrently": "^3.6.1",
    "nodemon": "^1.18.3",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^7.1.0"
  }
}
